print "Porovnani stejnych typu:\n"
if 1 <= 5 then
	print "1 <= 5\n"
end
if 2 == 2.0 then
	print "2 == 2.0\n"
end
if 1.3 <= 5.5 then
	print "1.3 <= 5.5\n"
end
if "ahoj" == "ahoj" then
	print "\"ahoj\" == \"ahoj\"\n"
end
if nil == nil then
	print "nil == nil\n"
end

print "\nPorovnani ruznych typu pomoci ==:\n"
if "ahoj" != 5 then
	print "\"ahoj\" != 5\n"
end
if "ahoj" != 5.5 then
	print "\"ahoj\" != 5.5\n"
end
if "ahoj" != nil then
	print "\"ahoj\" != nil\n"
end
if nil != 1 then
	print "nil != 1\n"
end
if nil != 1.5 then
	print "nil != 1.5\n"
end
